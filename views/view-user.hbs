<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View User</li>
  </ol>
</nav>

<div class="view-user p-5">
  {{#each rows}}
  <div class="row mb-5">
    <div class="col text-center">
      <h3>{{this.UserID}}</h3>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">UserID</th>
            <th scope="col">Password</th>
            <th scope="col">User Name</th>
            <th scope="col">Vai tro</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">{{this.UserID}}</th>
            <td>{{this.Pass}}</td>
            <td>{{this.userName}}</td>
            <td>{{this.Vai_tro}}</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  {{/each}}
</div>




